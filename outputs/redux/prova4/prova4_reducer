final prova4Reducer = combineReducers<Prova4State>([
	TypedReducer<Prova4State, UpdateProva4StateParam1Action>(_updateProva4StateParam1Action),
	TypedReducer<Prova4State, UpdateProva4StateParam2Action>(_updateProva4StateParam2Action),
	(state, action) => Prova4State(
		prova3: prova3Reducer(state.prova3, action),
	)
]);

Prova4State _updateProva4StateParam1Action(Prova4State state, UpdateProva4StateParam1Action action) {
	return state.copyWith(
		param1: action.value
	);
}

Prova4State _updateProva4StateParam2Action(Prova4State state, UpdateProva4StateParam2Action action) {
	return state.copyWith(
		param2: action.value
	);
}
