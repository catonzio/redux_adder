final prova2Reducer = combineReducers<Prova2State>([
	TypedReducer<Prova2State, UpdateProva2StateParam1Action>(_updateProva2StateParam1Action),
	TypedReducer<Prova2State, UpdateProva2StateParam2Action>(_updateProva2StateParam2Action),
]);

Prova2State _updateProva2StateParam1Action(Prova2State state, UpdateProva2StateParam1Action action) {
	return state.copyWith(
		param1: action.value
	);
}

Prova2State _updateProva2StateParam2Action(Prova2State state, UpdateProva2StateParam2Action action) {
	return state.copyWith(
		param2: action.value
	);
}
